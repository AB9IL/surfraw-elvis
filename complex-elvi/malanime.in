--flag=verygood:score:8
--flag=good:score:7
--flag=fine:score:6
--flag=average:score:5
--flag=bad:score:4
--flag=verybad:score:3
--flag=horrible:score:2
--flag=appalling:score:1

--enum=status:any:any,airing,finished,unaired
--collapse=status:any,0:airing,1:finished,2:unaired,3
