# Only for general searches like the myanimelist elvis.
www.youtube.com
www.youtube.com/results?
--description='Search YouTube'
# Singular form to be consistent with youtube's filters UI.
--enum=type:all:all,video,channel,playlist,movie,show

# Plural and singular forms to allow for easy use and remembering options.
--flag=all:type:all

--flag=video:type:video
--alias=videos:video:flag
--alias=vid:video:flag

--flag=channel:type:channel
--alias=channels:channel:flag

--flag=playlist:type:playlist
--alias=playlists:playlist:flag

--flag=movie:type:movie
--alias=movies:movie:flag

--flag=show:type:show
--alias=shows:show:flag

--collapse=type:all,:video,EgIQAQ%253D%253D:channel,EgIQAg%253D%253D:playlist,EgIQAw%253D%253D:movie,EgIQBA%253D%253D:show,EgIQBQ%253D%253D
--map=type:sp:no
--query-parameter=search_query
